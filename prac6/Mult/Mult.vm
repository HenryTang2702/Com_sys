// Calculates a = x*y
// a is a local variable
// x & y are static variables

// Put your code here.
push constant 0
pop local 0

push static 0 
pop argument 0

push static 1
pop argument 1


label LOOP
  // Check if y is less than or equal to 0
  push static 1
  push constant 0
  leq
  if-goto END

  // Add x to a
  push local 0
  push static 0
  add
  pop local 0

  // Decrement y
  push static 1
  push constant 1
  sub
  pop static 1

  // Repeat the loop
  goto LOOP


label END
goto END